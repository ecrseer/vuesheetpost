{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:3000/",
        "webRoot": "${workspaceFolder}/src",
        "breakOnLoad": true,
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        },
        "runtimeExecutable": "/usr/lib/chromium-browser/chromium-browser"
      } ,
      {
        "type": "node", 
        "name": "Attach Node.js",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
      }
    ]
  }